{
	"arrowParens": "always",
	"bracketSpacing": true,
	"endOfLine": "lf",
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"parser": "typescript",
	"plugins": ["prettier-plugin-svelte"],
	"printWidth": 80,
	"quoteProps": "preserve",
	"semi": false,
	"singleQuote": true,
   "trailingComma": "es5",
	"useTabs": true
}
